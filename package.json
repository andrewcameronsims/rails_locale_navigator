{
	"name": "localenavigator",
	"publisher": "andrewcameronsims",
	"displayName": "Rails Locale Navigator",
	"description": "Command to open or create Rails locale files",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Keymaps"
	],
	"activationEvents": [
		"onCommand:localenavigator.railsLocale"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "localenavigator.railsLocale",
				"title": "Rails Locale"
			}
		],
		"keybindings": [
			{
				"command": "localenavigator.railsLocale",
				"key": "ctrl+alt+l",
				"mac": "shift+cmd+l"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/js-yaml": "^4.0.0",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"js-yaml": "^4.0.0"
	}
}
